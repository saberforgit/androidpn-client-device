    <application
        android:icon="@drawable/pander"
        android:label="@string/app_name" >
        <activity
            android:name="org.androidpn.demoapp.DemoAppActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.devicemanager" />
